{
    "manifest_version": 3,
    "name": "OnFuse Wallet",
    "version": "1.0.0",
    "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
    "action": {
      "default_popup": "index.html", 
      "default_icon": {
        "32": "logo.png"
      }
    },
    "permissions": [
      "scripting"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ]
  }
  